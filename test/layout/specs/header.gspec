x @objects
    mainHeader    css   .MainHeader__header
    logo          css   .MainHeader__logo

= Header =
    @on *
        mainHeader

    @on desktop
        logo:
            width 238px
            centered horizontally inside mainHeader
            inside mainHeader 20px top

    @on mobile
        logo:
            inside mainHeader 20px left right top
            centered horizontally inside mainHeader
